{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup/index.html",
      "default_title": "Notion Saver"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+Shift+E",
            "mac": "Alt+Shift+E"
         }
      },
      "add-highlights": {
         "description": "Add highlights to your Notion Page",
         "suggested_key": {
            "default": "Ctrl+Shift+E",
            "mac": "Command+Shift+E"
         }
      }
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-...='; object-src 'self'",
   "description": "Save links to your Notion databases",
   "icons": {
      "128": "./assets/icon128.png",
      "48": "./assets/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Save to Notion",
   "permissions": [ "activeTab", "storage", "cookies", "contextMenus", "unlimitedStorage", "https://*.notion.so/*" ],
   "short_name": "Save to Notion",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.9.9"
}
