{
   "background": {
      "scripts": [ "v1/sc_ext_background_v1.js", "v2/sc_ext_background.js" ]
   },
   "content_scripts": [ {
      "js": [ "v1/sc_ext_content_v1.js", "v2/sc_ext_content.js" ],
      "matches": [ "https://*.facebook.com/*", "https://*.messenger.com/*" ]
   } ],
   "description": "Share your screen in video calls on Facebook, Messenger and Workplace",
   "externally_connectable": {
      "matches": [ "https://*.facebook.com/*", "https://*.messenger.com/*" ]
   },
   "icons": {
      "128": "icon144.png",
      "16": "icon29.png",
      "48": "icon50.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "34",
   "name": "Facebook Screen Sharing",
   "permissions": [ "desktopCapture", "notifications", "https://*.facebook.com/", "https://*.messenger.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.10.4"
}
